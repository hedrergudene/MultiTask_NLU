{
    "HuggingFace_model":"xlm-roberta-base",
    "spaCy_model": "en_core_web_sm",
    "scheme":"IB",
    "dropout":0.25,
    "batch_size":128,
    "dim":256,
    "epochs":5,
    "dataloader_num_workers":4,
    "gradient_accumulation_steps":4,
    "gamma":2.0,
    "learning_rate":1e-4,
    "weight_decay":1e-3,
    "warmup_steps_factor":2,
    "evaluation_steps_per_epoch":1,
    "early_stopping":5,
    "scheduler_mode":"min",
    "verbose_steps":10,
    "filepath":"output/",
    "device":"cuda",
    "fp16":1,
    "seed":123
}
